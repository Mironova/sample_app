<% provide(:title, t('users.new.signup')) %>
<h1><%= t('users.new.signup')%></h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@user) do |f| %>

      <%= render 'fields', f: f %>

      <%= f.submit t('users.new.btn'), class: "btn btn-large btn-primary" %>

    <% end %>
  </div>
</div>


